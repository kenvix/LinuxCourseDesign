sqlite> .tables[Kschema ranks[K[K[K[K[Kdaily_rank_by_type
CREATE VIEW "daily_rank_by_type" AS SELECT
	t1.type, 
	t1.subtype, 
	t1.userid, 
	t1.name, 
	t1.studentid,
	t1.date, 
	(
		SELECT COUNT(*) 
		FROM everything AS t2
		WHERE 
			t2.userid = t1.userid 
			AND t2.type = t1.type 
			AND (t2.subtype = t1.subtype OR (t1.subtype IS NULL AND t2.subtype IS NULL))
			and t2.date = t1.date
	) AS subtype_count
FROM
	everything AS t1
GROUP BY
	subtype, 
	type, 
	userid,
	date
ORDER BY
	subtype_count DESC
/* daily_rank_by_type(type,subtype,userid,name,studentid,date,subtype_count) */;
